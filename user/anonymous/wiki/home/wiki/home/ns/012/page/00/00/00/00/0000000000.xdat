<script>
var special_wikis = {
  wikidata: 'www.wikidata.org',
  commons: 'commons.wikimedia.org',
  species: 'species.wikimedia.org',
  meta: 'meta.wikimedia.org',
  incubator: 'incubator.wikimedia.org',
  mediawiki: 'www.mediawiki.org',
  wikimediafoundation: 'wikimediafoundation.org',
  other: ''
};

function cbo_selected_value (cbo_id) {
  var cbo = document.getElementById(cbo_id);
  return cbo.options[cbo.selectedIndex].value;
}

function enable_disable_langs_list () {
  document.getElementById('xowa_langs_list').disabled = (cbo_selected_value('xowa_types_list') in special_wikis);
}

function enable_disable_file_input () {
  document.getElementById('xowa_dump_file').disabled = cbo_selected_value('xowa_import_method') === 'download';
  document.getElementById('xowa_dump_file_button').disabled = cbo_selected_value('xowa_import_method') === 'download';
}

function read_domain () {
  var wiki = cbo_selected_value('xowa_types_list');
  if (wiki in special_wikis) {
    return special_wikis[wiki];
  }
  return cbo_selected_value('xowa_langs_list') + '.' + wiki + '.org'; // EX: en.wikipedia.org
}

function read_file () {
  return cbo_selected_value('xowa_import_method') === 'download' ?
    '' :
    document.getElementById('xowa_dump_file').value;
}


function generate_script (domain, file, unzip, category, search) {
  var script = '';

  // import from dump-file
  if (file !== '') {
    script += '// import wiki from dump file\n';
    var unzip_str = unzip ? "unzip" : "";
    script += 'app.setup.cmds.cmd_add("wiki.dump_file", "' + file + '", "' + domain  + '", "' + unzip_str + '");\n\n';
  }

  // import by downloading
  else {
    // do not generate download wiki script if category update only
    if (   category !== 'update_version2'
        && search   !== 'update_version2') {
      script += '// import wiki by downloading file\n'
             +  "app.setup.cmds.dump_add_many('" + domain + "', 'latest', 'pages-articles', 'wiki.download'"
      if (unzip) {
        script += ", 'wiki.unzip'";
      }      
      script += ", 'wiki.import');\n\n";
    }
  }

  // category version 2 selected (either create_version2 or update_version2)
  if (category !== 'create_version1') {
    var category_download_snippet = ", 'wiki.category2.download.page_props', 'wiki.category2.download.categorylinks'";
    if (file !== '') { // dumpfile not selected from file-system; assume that user has category files locally
      category_download_snippet = "";
    }
    script += '// upgrade category to version 2\n'
           + "app.setup.cmds.dump_add_many"
           +   "( '" + domain + "'"
           +   ", 'latest', ''"
           +   category_download_snippet
           +   ", 'wiki.category2.build');\n\n"
  }

  // search version 2 selected (either create_version2 or update_version2)
  if (search === 'update_version2') {
    script += '// upgrade search to version 2\n'
           + "app.setup.cmds.dump_add_many"
           +   "( '" + domain + "'"
           +   ", 'latest', ''"
           +   ", 'wiki.search2.build');\n\n"
  }

  return script;
}

function run_script (type) {
  var script;
  if (type < 2) {
    script = generate_script(
      read_domain(),
      read_file(),
      cbo_selected_value('xowa_unzip') === 'unzip',
      cbo_selected_value('xowa_category_list'),
      cbo_selected_value('xowa_search_list')
    );
  } else {
    script = document.getElementById('xowa_script_code').value;
  }
  if (type !== 1) {
    xowa_exec('scripts_exec', script + 'app.setup.cmds.cmd_add("msg.ok", "script done"); app.setup.cmds.run;');
  } else {
    document.getElementById('xowa_script_code').value += script;
  }
}

function init () {
  document.getElementById('xowa_dump_file_button').onclick = function () {
    xowa_io_select('file', 'xowa_dump_file', '');
  };
  document.getElementById('xowa_types_list').onchange = enable_disable_langs_list;
  document.getElementById('xowa_import_method').onchange = enable_disable_file_input;
  document.getElementById('xowa_exec_direct').onclick = function () {
    run_script(0);
  };
  document.getElementById('xowa_exec_generate').onclick = function () {
    run_script(1);
  };
  document.getElementById('xowa_exec_generated').onclick = function () {
    run_script(2);
  };
  enable_disable_langs_list();
  enable_disable_file_input();
}

init();
</script>
